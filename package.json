  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --host=0.0.0.0 --port=8080",
    "clean": "rm -rf lib dist local es *.d.ts",
    "typing": "dts-generator --name medium-draft --project ./ --out ./medium-draft.d.ts",
    "dist": "webpack --mode production",
    "build:es": "tsc --outDir ./es --target es6 --module es6",
    "build": "npm run clean && tsc && npm run build:es && npm run typing && npm run dist",
    "local": "mkdir -p local && cp -r lib dist package.json local",
    "docs": "typedoc --out docs ./src/index.ts --mode file --name 'Medium Draft'",
    "prepublishOnly": "npm run build"
  },
